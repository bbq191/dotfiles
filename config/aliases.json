{
  "navigation": {
    "..": "cd ..",
    "...": "cd ../..",
    "....": "cd ../../..",
    "~": "cd ~",
    "projects": "cd ~/Projects",
    "dotfiles": "cd ~/dotfiles"
  },
  "file_operations": {
    "bash": {
      "ll": "ls -alF",
      "la": "ls -A",
      "l": "ls -CF",
      "tree": "find . -print | sed -e \"s;[^/]*/;|____;g;s;____|; |;g\""
    },
    "powershell": {
      "ll": "Get-ChildItem -Force",
      "la": "Get-ChildItem -Force",
      "l": "Get-ChildItem",
      "tree": "tree /f"
    }
  },
  "git": {
    "gs": "git status",
    "ga": "git add",
    "gaa": "git add --all",
    "gc": "git commit -m",
    "gca": "git commit --amend",
    "gp": "git push",
    "gl": "git log --oneline",
    "gco": "git checkout",
    "gb": "git branch",
    "gd": "git diff",
    "gds": "git diff --staged"
  },
  "development": {
    "bash": {
      "py": "python",
      "pip": "python -m pip",
      "serve": "python -m http.server 8000",
      "json": "python -m json.tool"
    },
    "powershell": {
      "py": "python",
      "pip": "python -m pip",
      "serve": "python -m http.server 8000",
      "json": "ConvertTo-Json"
    }
  },
  "system": {
    "bash": {
      "open": "explorer",
      "which": "which",
      "grep": "grep --color=auto"
    },
    "powershell": {
      "open": "Invoke-Item",
      "which": "Get-Command",
      "grep": "Select-String"
    }
  }
}